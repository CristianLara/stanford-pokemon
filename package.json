{
  "name": "stanford-pokemon",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://cristianlara.github.io/stanford-pokemon",
  "dependencies": {
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "react-sound": "^1.2.0",
    "styled-components": "^3.2.3",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.0",
    "gh-pages": "^1.1.0"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "test": "./test",
            "underscore": "lodash",
            "graphics": "./graphics"
          }
        }
      ]
    ]
  }
}
